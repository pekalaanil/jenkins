pipeline{
    agent none
    stages{
        stage('Download Code'){
            steps{
                 echo 'This is Downloading code stage'
            }
        }
        stage('Build Stage'){
            steps{
                 echo 'Build Stage is processing'
            }
        }
        stage('Compile'){
             echo 'Compile Stage is processing'
        }
        stage('Deploy'){
             echo 'Deploy Stage is processing'
        }
        stage('Final Stage'){
             echo 'Final Stage is processing'
        }
    }
}