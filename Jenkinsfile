pipeline{
    agent none
    stages{
        stage('Download Code'){
            steps{
                 echo 'This is Downloading code stage'
            }
        }
        stage('Build Stage'){
            steps{
                 echo 'Build Stage is processing'
            }
        }
        stage('Compile Stage'){
            steps{
                 echo 'Compile Stage is processing'
            }
        }
        stage('Docker Image'){
            steps{
                echo 'Docker Stage...'
            }
        }
        stage('Deploy Stage'){
            steps{
                 echo 'Deploy Stage is processing'
            }
        }

    }
}