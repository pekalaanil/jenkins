pipeline{
    agent any
    stages{
        stage('Download Code'){
            steps{
                sh 'echo This is Downloading code stage'
            }
        }
        stage('Build Stage'){
            steps{
                sh 'echo Build Stage is processing'
            }
        }
        stage('Compile'){
            sh 'echo Compile Stage is processing'
        }
        stage('Deploy'){
            sh 'echo Deploy Stage is processing'
        }
        stage('Final Stage'){
            sh 'echo Final Stage is processing'
        }
    }
}