pipeline{
    agent any
    stages{
        stage('Download Code'){
            steps{
                echo 'This is Downloading code stage'
            }
        }
        stage('Build Stage'){
            steps{
                echo 'Build Stage is processing'
            }
        }
        stage('Compail'){
            echo 'this is compail stage'
        }
        stage('Deploy'){
            echo 'this is deploy stage'
        }
        stage('Final Stage'){
            echo 'This is final stage.'
        }
    }
}